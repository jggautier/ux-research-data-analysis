                            "value": [
                                {
                                    "dsDescriptionValue": {
                                        "typeName": "dsDescriptionValue",
                                        "multiple": false,
                                        "typeClass": "primitive",
                                        "value": "Although several measures of state courts of last resort professionalization levels have been devised, the Squire (2008) index has been the most commonly employed in recent years (Yanus and Gray 2017, 284). A significant limitation of it, however, is that it only uses data from a single year, 2004 (Frost and Lindquist 2010, 776). In this paper we update the Squire state courts of last resort professionalization index using data for 2019. Because of a change in the way state courts of last resort docket control data are currently presented, we also recode the 2004 index to make it directly comparable with the 2019 version."
                                    },
                                    "dsDescriptionDate": {
                                        "typeName": "dsDescriptionDate",
                                        "multiple": false,
                                        "typeClass": "primitive",
                                        "value": "2020-06"
                                    }
